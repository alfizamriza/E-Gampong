{
    "name": "user/e-gampong",
    "description": "E-Gampong - Sistem Pembuatan Surat Desa",
    "license": "proprietary",
    "require": {
        "doctrine/instantiator": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml",
        "phpstan": "vendor/bin/phpstan analyse --level=max src/",
        "phpcs": "vendor/bin/phpcs --standard=PSR12 src/"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
